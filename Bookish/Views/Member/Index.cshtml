@namespace Bookish
@model List<Models.MemberModel>
@{
    ViewData["Title"] = "Members";
}


   <h1>All Library Members</h1>

   <table>
       <thead>
           <tr>
               <th>Id</th>
               <th>First Name</th>
               <th>Last Name</th>
               <th>Date Of Birth</th>

       
           </tr>
       </thead>
       <tbody>
        @foreach(var item in Model)
        {<tr>
            <td>@item.Id</td>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.DateOfBirth</td>
        </tr>
        }
       </tbody>
   </table> 

   <a href="~/Member/Add">Add A Member</a>
